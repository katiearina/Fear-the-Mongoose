<h3>Current Articles</h3>

<div class="current-articles">
  {{#each articles}}
    <div class="card w-50">
    <div class="card-block">
      <a href="{{this.link}}" target="_blank">
        <h4 class="card-title">{{this.title}}</h4>
      </a>
      <p class="card-text">Comment!</p>
      <p class="card-text">Save!</p>
    </div>
    </div>
  {{/each}}
</div>